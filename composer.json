{
  "name": "okriiza/laravel-api-response-formatter",
  "description": "a simple package Format API responses throughout your Laravel application",
  "keywords": [
    "okriiza",
    "api-response-formatter",
    "laravel api response formatter",
    "laravel api response formatter helpers"
  ],
  "homepage": "https://github.com/okriiza/laravel-api-response-formatter",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Rendi Okriza",
      "email": "okriizaa@gmail.com",
      "homepage": "https://okriiza.my.id",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0| ^10.0",
    "phpunit/phpunit": "^7.0|^8.0|^9.0|^9.5|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Okriiza\\ApiResponseFormatter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Okriiza\\ApiResponseFormatter\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Okriiza\\ApiResponseFormatter\\ApiResponseServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
